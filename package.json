{
  "name": "rubic",
  "version": "1.0.3",
  "scripts": {
    "postinstall": "node scripts/patch-webpack.js",
    "start:base": "npm run prebuild && node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --host local.rubic.exchange --port 4224 --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key --allowed-hosts all",
    "start": "npm run start:base --",
    "start:local": "npm run start:base -- --configuration=local",
    "start:dev": "npm run start:base -- --configuration=dev",
    "start:dev2": "npm run start:base -- --configuration=dev2",
    "start:dev3": "npm run start:base -- --configuration=dev3",
    "start:dev4": "npm run start:base -- --configuration=dev4",
    "start:dev5": "npm run start:base -- --configuration=dev5",
    "start:dev6": "npm run start:base -- --configuration=dev6",
    "start:prod": "npm run start:base -- --configuration=production",
    "start:stage": "npm run start:base -- --configuration=stage",
    "start:sdk": "npm run start:base -- --configuration=sdk",
    "start:prod-api": "npm run start:base -- --configuration=prod-api",
    "prebuild": "node scripts/build-timestamp.js",
    "build:base": "npm run prebuild && node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build",
    "build": "npm run build:base -- --configuration=production",
    "build:dev": "npm run build:base",
    "build:dev2": "npm run build:base -- --configuration=dev2",
    "build:dev3": "npm run build:base -- --configuration=dev3",
    "build:dev4": "npm run build:base -- --configuration=dev4",
    "build:dev5": "npm run build:base -- --configuration=dev5",
    "build:dev6": "npm run build:base -- --configuration=dev6",
    "build:stage": "npm run build:base -- --configuration=stage",
    "build:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/rubic/stats.json",
    "test": "ng test",
    "test:ci": "ng test --karma-config=src/karma-ci.conf.js --no-progress",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint-css": "stylelint '**/*.scss'",
    "pre-commit": "npx lint-staged",
    "prepare": "husky install",
    "unused-css": "ngx-unused-css",
    "compodoc": "compodoc -p tsconfig.doc.json -s",
    "precompile": "node scripts/build-timestamp.js",
    "compile": "npm run precompile && tsc",
    "install:local-sdk": "ng cache clean && yarn add --force file:../rubic-monorepo/dist/packages/core file:../rubic-monorepo/dist/packages/web3 file:../rubic-monorepo/dist/packages/sdk",
    "install:prod-sdk": "yarn add @cryptorubic/core @cryptorubic/web3 @cryptorubic/sdk",
    "install:version": "sh -c 'ver=\"$1\"; yarn add @cryptorubic/core@\"$ver\" @cryptorubic/web3@\"$ver\" @cryptorubic/sdk@\"$ver\"' --"
  },
  "private": true,
