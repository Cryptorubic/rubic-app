{
  "name": "rubic",
  "version": "1.0.3",
  "scripts": {
    "start": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json --host local.rubic.exchange --port 4224 --disable-host-check  --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "start:dev2": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --configuration=dev2 --proxy-config proxy.conf.json --host local.rubic.exchange --port 4224 --disable-host-check  --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "start:prod-api": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --configuration=prod-api --proxy-config proxy.conf.json --host local.rubic.exchange --port 4224 --disable-host-check  --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "start-with-no-reload": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json --host local.rubic.exchange --port 4224 --disable-host-check  --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key --no-live-reload",
    "start-with-no-reload:second": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json --host local.rubic.exchange --port 2442 --disable-host-check  --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key --no-live-reload",
    "postinstall": "node scripts/patch-webpack.js",
    "build": "ng build --configuration production && npm run compress-images",
    "build:dev": "ng build && npm run compress-images",
    "build:dev2": "ng build --configuration dev2 && npm run compress-images",
    "build:stage": "ng build --configuration stage && npm run compress-images",
    "build:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/rubic/stats.json && npm run compress-images",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint-css": "stylelint '**/*.scss'",
    "pre-commit": "npx lint-staged",
    "prepare": "husky install",
    "unused-css": "ngx-unused-css",
    "compress-images": "node scripts/compress-images.mjs",
    "compodoc": "compodoc -p tsconfig.doc.json -s",
    "compile": "tsc"
  },
